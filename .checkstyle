<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="true" sync-formatter="false">
  <local-check-config name="OmegaT config" location="config/checkstyle/checkstyle.xml" type="project" description="">
    <additional-data name="protect-config-file" value="true"/>
  </local-check-config>
  <fileset name="all" enabled="true" check-config-name="OmegaT config" local="true">
    <file-match-pattern match-pattern="." include-pattern="true"/>
  </fileset>
  <filter name="WriteProtectedFiles" enabled="true"/>
  <filter name="DerivedFiles" enabled="true"/>
  <filter name="UnOpenedFiles" enabled="true"/>
  <filter name="FilesFromPackage" enabled="true">
    <filter-data value="src/gen"/>
  </filter>
</fileset-config>
